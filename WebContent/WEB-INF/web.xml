<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ProjetoWeb</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>ProdutoController</servlet-name>
    <servlet-class>br.com.coti.control.ProdutoController</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>ProdutoController</servlet-name>
    <url-pattern>/produto</url-pattern>
  </servlet-mapping>
    
  <servlet>
    <servlet-name>ClienteController</servlet-name>
    <servlet-class>br.com.coti.control.ClienteController</servlet-class>
  </servlet>
  
   <servlet-mapping>
    <servlet-name>ClienteController</servlet-name>
    <url-pattern>/cliente</url-pattern>
    
  </servlet-mapping>
</web-app>